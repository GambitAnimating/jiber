{
  "name": "jiber-client",
  "version": "0.0.1",
  "description": "Synced data store",
  "main": "dist/server/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run compile && npm run bundle && npm run compress",
    "compile": "tsc",
    "bundle": "rollup -c",
    "compress": "uglifyjs -c -m -o dist/jiber-client.bundle.min.js dist/jiber-client.bundle.js"
  },
  "author": "Jacob Grahn",
  "license": "MIT",
  "devDependencies": {
    "rollup": "^0.49.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "typescript": "^2.5.2",
    "uglify-es": "^3.0.15"
  },
  "dependencies": {
    "jiber-core": "^0.0.1"
  }
}
